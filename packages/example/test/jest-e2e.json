{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@ahas/test-lib/(.*)": "<rootDir>/../libs/test-lib/src/$1",
    "@ahas/test-lib": "<rootDir>/../libs/test-lib/src"
  }
}
